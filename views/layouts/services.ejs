<!-- Services Section -->
<section class="page-section" id="services">
    <div class="container relative">

        <h2 class="section-title font-alt mb-70 mb-sm-40">
            Services
        </h2>

        <!-- Nav tabs -->
        <ul class="nav nav-tabs tpl-alt-tabs font-alt pt-30 pt-sm-0 pb-30 pb-sm-0">

            <% navtabs.tabs.forEach(function(bar, index){ %>

            <% if (index==0) { %>
            <li class="active">
                <a href='<%= bar.tag %>' data-toggle="tab">

                    <div class="alt-tabs-icon">
                        <span class='<%= bar.icon %>'></span>
                    </div>
                    <%= bar.tittle %>
                </a>
            </li>
            <% } else { %>
            <li>
                <a href='<%= bar.tag %>' data-toggle="tab">

                    <div class="alt-tabs-icon">
                        <span class='<%= bar.icon %>'></span>
                    </div>
                    <%= bar.tittle %>
                </a>
            </li>
            <% } %>
            <% }); %>

            <!-- End Nav tabs -->
        </ul>

        <!-- Tab panes -->
        <div class="tab-content tpl-tabs-cont">
            <% navtabs.sections.forEach(function(section, index){ %>
            <% if (index==0) { %>
            <!-- Service Item -->
            <div class="tab-pane fade in active" id='<%= section.tag %>'>
                <div class="section-text">
                    <div class="row">
                        <div class="col-md-4 mb-md-40 mb-xs-30">
                            <blockquote class="mb-0">
                                <p> <%= section.quote.blockquote %> </p>
                                <footer> <%= section.quote.author %> </footer>
                            </blockquote>
                        </div>
                        <div class="col-md-4 col-sm-6 mb-sm-50 mb-xs-30">
                            <%= section.paragraphOne %>
                        </div>
                        <div class="col-md-4 col-sm-6 mb-sm-50 mb-xs-30">
                            <%= section.paragraphTwo %>
                        </div>
                    </div>
                </div>
                <% if (section.pricing) { %>
                <%- partial(section.pricing) %>
                <% } %>
            </div>
            <!-- End Service Item -->
            <% } else { %>
            <!-- Service Item -->
            <div class="tab-pane fade" id='<%= section.tag %>'>
                <div class="section-text">
                    <div class="row">
                        <div class="col-md-4 mb-md-40 mb-xs-30">
                            <blockquote class="mb-0">
                                <p> <%= section.quote.blockquote %> </p>
                                <footer> <%= section.quote.author %> </footer>
                            </blockquote>
                        </div>
                        <div class="col-md-4 col-sm-6 mb-sm-50 mb-xs-30">
                            <%= section.paragraphOne %>
                        </div>
                        <div class="col-md-4 col-sm-6 mb-sm-50 mb-xs-30">
                            <%= section.paragraphTwo %>
                        </div>
                    </div>
                </div>
                <% if (section.pricing) { %>
                <%- partial(section.pricing, {stacks: section.stacks}) %>
                <% } %>
            </div>
            <!-- End Service Item -->
            <% } %>
            <% }); %>

        </div>
        <!-- End Tab panes -->

    </div>
</section>
<!-- End Services Section -->